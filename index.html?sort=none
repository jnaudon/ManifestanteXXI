$(function() {
  $(".explore-icons a").tooltip();
  
  $("#explore-header .drawer-pull").click(function(e){
    var drawer = $('#explore-drawer'),
        content = drawer.find('.drawer-content');
        
    if (drawer.hasClass('expanded')) {
      content.slideUp('slow', function(){
        drawer.removeClass('expanded').addClass('collapsed');
      });    
      document.cookie="exploreHeaderDrawerClosed=1; path=/";        
    } else {
      content.slideDown('slow', function() {
        drawer.removeClass('collapsed').addClass('expanded');
      });
      document.cookie='exploreHeaderDrawerClosed=0; path=/';      
    }
  });
});
</script><div class="container"><div class="explore-content"><div class="explore-menubar"><div class="btn-toolbar clearfix"><div class="types-toolbar fl"><div class="btn-group categories-menu"><a data-toggle="dropdown" class="btn btn-small dropdown-toggle"><i class="bg-icon category-all"></i>&nbsp;Everything&nbsp;<span class="caret"></span></a><ul class="dropdown-menu"><li class="active"><a href="/tag/type-id/"><i class="icon-category-all"></i>Everything</a></li><li><a href="/tag/type-id/category-food/?sort=none"><i class="icon-category-food"></i>Food</a></li><li><a href="/tag/type-id/category-living/?sort=none"><i class="icon-category-living"></i>Living</a></li><li><a href="/tag/type-id/category-outside/?sort=none"><i class="icon-category-outside"></i>Outside</a></li><li><a href="/tag/type-id/category-play/?sort=none"><i class="icon-category-play"></i>Play</a></li><li><a href="/tag/type-id/category-technology/?sort=none"><i class="icon-category-technology"></i>Technology</a></li><li><a href="/tag/type-id/category-workshop/?sort=none"><i class="icon-category-workshop"></i>Workshop</a></li></ul></div><div class="btn-group channels-menu"><a data-toggle="dropdown" class="btn btn-small dropdown-toggle">All Channels&nbsp;<span class="caret"></span></a><ul class="dropdown-menu dropdown-scroll"><li><a href="/tag/type-id/category-technology/channel-3D-Printing/?sort=none">3D Printing</a></li><li><a href="/tag/type-id/category-outside/channel-Birding/?sort=none">Birding</a></li><li><a href="/tag/type-id/category-play/channel-airsoft/?sort=none">Airsoft</a></li><li><a href="/tag/type-id/category-technology/channel-apple/?sort=none">Apple</a></li><li><a href="/tag/type-id/category-technology/channel-arduino/?sort=none">Arduino</a></li><li><a href="/tag/type-id/category-living/channel-art/?sort=none">Art (living)</a></li><li><a href="/tag/type-id/category-technology/channel-art/?sort=none">Art (technology)</a></li><li><a href="/tag/type-id/category-technology/channel-assistive-