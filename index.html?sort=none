$(function(){
  $('.btn-close').click(function(e){
    e.preventDefault();
    var url = window.location.href;
    
    if ($(this).hasClass('tag-remove')) {
      var urlKeywordToken = $(this).data('url-keyword-token');
      url = url.replace('/'+urlKeywordToken+'/', '/');
      window.location = url;
    }
    
    if ($(this).hasClass('search-remove')) {
      window.location = window.location.pathname;
    }  
  });
  
  $('.items-per-page-link').click(function(e){
    e.preventDefault();
    var href = $(this).attr('href'),
        numberPerPage = $(this).data('count');
    document.cookie = 'ibleItemsPerPage='+numberPerPage+';';
    window.location.reload(true);
  });
});
